{
	"name": "@ai-stats/sdk",
	"description": "AI Stats Gateway SDK for TypeScript/JavaScript",
	"version": "0.2.0",
	"type": "module",
	"private": false,
	"license": "MIT",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"scripts": {
		"clean": "rimraf dist src/gen",
		"gen": "pnpm -w openapi:gen:ts",
		"build": "tsc -p tsconfig.build.json",
		"test": "echo \"No automated tests defined yet.\"",
		"pack:dry": "npm pack --dry-run",
		"test:smoke": "tsx ./scripts/smoke.ts"
	},
	"devDependencies": {
		"typescript": "^5.6.3",
		"tsx": "^4.7.0",
		"rimraf": "^6.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DanielButler1/AI-Stats.git"
	},
	"publishConfig": {
		"access": "public"
	}
}
